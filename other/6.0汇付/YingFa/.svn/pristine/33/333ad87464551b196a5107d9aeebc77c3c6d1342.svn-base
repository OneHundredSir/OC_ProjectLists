#import <Foundation/Foundation.h>

@interface Payment : NSObject

@property (nonatomic, copy) NSString *title;

@property (nonatomic, assign) NSInteger paymentId;// 借款标ID

@property (nonatomic, copy) NSString *smallImageFilename;// 借款标类型

@property (nonatomic, assign) CGFloat amount;// 借款金额

@property (nonatomic, assign) NSInteger period;// 借款期数	当借款期限单位为年时期数要乘以12

@property (nonatomic, assign) NSInteger periodUnit;// 借款期限单位	-1为年 0为月 1为天

@property (nonatomic, copy) NSString *repaymentTime; // 还款时间
//{"date":20,"day":3,"hours":22,"minutes":10,"month":7,"nanos":0,"seconds":13,"time":1408543813000,"timezoneOffset":-480,"year":114}


@property (nonatomic, assign) NSInteger status;// 

@end
